    <script>
        function createDucks() {
            for (let i = 0; i < 3; i++) {
                const duck = document.createElement('div');
                duck.className = 'duck';
                duck.innerHTML = '🦆';
                duck.style.left = Math.random() * 90 + 5 + '%';
                duck.style.top = Math.random() * 90 + 5 + '%';
                duck.style.animationDelay = Math.random() * 5 + 's';
                document.body.appendChild(duck);
            }
        }
        createDucks();
    </script>
