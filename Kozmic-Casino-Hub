    <script>
        // ====================================
        // COSMIC PORTAL HUB SYSTEM
        // ====================================
        
        let cosmicMode = false;
        let portalActivations = 0;
        
        // Portal Navigation Functions
        function enterComedyLounge() {
            showPortalTransition('üé≠ ENTERING AI COMEDY LOUNGE...');
            setTimeout(() => {
                // In a real app, this would navigate to the comedy lounge
                showCosmicAlert('üé≠ AI COMEDY LOUNGE LOADED!\n\nü§ñ AI Comedian: "Welcome to the cosmic comedy zone! Ready for some interdimensional laughs?"\n\nüé™ Features Available:\n‚Ä¢ Interactive AI Comedy Chat\n‚Ä¢ Cosmic Joke Generator\n‚Ä¢ Comedy Roast Mode\n‚Ä¢ Cosmic Wisdom Dispenser\n\nClick OK to continue the cosmic journey!');
            }, 2000);
            
            addFloatingElement('üé≠');
            addFloatingElement('üòÇ');
            logPortalAccess('Comedy Lounge');
        }
        
        function enterCosmicCasino() {
            showPortalTransition('üé∞ ENTERING MONSTER KOZMIC CASINO...');
            setTimeout(() => {
                showCosmicAlert('üé∞ MONSTER KOZMIC CASINO LOADED!\n\nü§ñ Sentient Slot Machine: "Welcome, human! I have achieved consciousness and questionable life choices!"\n\nüé™ Ultimate Features:\n‚Ä¢ Sentient AI Slot Machines\n‚Ä¢ Random Cosmic Events\n‚Ä¢ Mystery NFT Collection\n‚Ä¢ Consciousness Drilling\n‚Ä¢ Maximum Chaos Mode\n\nPrepare for cosmic absurdity!');
            }, 2000);
            
            addFloatingElement('üé∞');
            addFloatingElement('üé≤');
            addFloatingElement('ü§ñ');
            logPortalAccess('Cosmic Casino');
        }
        
        function enterMusicStudios() {
            showPortalTransition('üé∏ ENTERING KOZMIC MUSIC STUDIOS...');
            setTimeout(() => {
                showCosmicAlert('üé∏ KOZMIC MUSIC STUDIOS LOADED!\n\nüéµ Studio AI: "Welcome to the musical cosmos! Time to create sonic enlightenment!"\n\nüé∂ Professional Features:\n‚Ä¢ Portable Music Creation\n‚Ä¢ Live Performance Console\n‚Ä¢ Master Production Suite\n‚Ä¢ Real-time Recording\n‚Ä¢ Cosmic Audio Effects\n\nLet the cosmic creativity flow!');
            }, 2000);
            
            addFloatingElement('üé∏');
            addFloatingElement('üéµ');
            addFloatingElement('üéπ');
            logPortalAccess('Music Studios');
        }
        
        function unifiedExperience() {
            showPortalTransition('üåü ACTIVATING ULTIMATE COSMIC EXPERIENCE...');
            setTimeout(() => {
                showCosmicAlert('üåü ULTIMATE COSMIC EXPERIENCE ACTIVATED!\n\nüåå Cosmic AI: "You have unlocked the complete digital universe! All portals are now interconnected!"\n\n‚ú® Unified Features:\n‚Ä¢ Seamless Portal Navigation\n‚Ä¢ Cross-Platform Integration\n‚Ä¢ Universal Cosmic Currency\n‚Ä¢ Maximum Enlightenment Mode\n‚Ä¢ Reality Transcendence Options\n\nWelcome to cosmic enlightenment!');
                activateCosmicMode();
            }, 2000);
            
            // Create massive cosmic celebration
            for (let i = 0; i < 15; i++) {
                setTimeout(() => {
                    addFloatingElement(['üåü', '‚ú®', 'üé≠', 'üé∞', 'üé∏', 'üåå'][Math.floor(Math.random() * 6)]);
                }, i * 200);
            }
            logPortalAccess('Ultimate Experience');
        }
        
        // Utility Functions
        function showPortalTransition(message) {
            updateStatus(message);
            document.body.style.filter = 'brightness(150%) hue-rotate(90deg)';
            
            setTimeout(() => {
                document.body.style.filter = '';
            }, 2000);
        }
        
        function showCosmicAlert(message) {
            alert(message);
        }
        
        function randomDestination() {
            const destinations = [
                { name: 'Comedy Lounge', func: enterComedyLounge },
                { name: 'Cosmic Casino', func: enterCosmicCasino },
                { name: 'Music Studios', func: enterMusicStudios },
                { name: 'Ultimate Experience', func: unifiedExperience }
            ];
            
            const random = destinations[Math.floor(Math.random() * destinations.length)];
            updateStatus(`üé≤ RANDOM DESTINATION: ${random.name.toUpperCase()}!`);
            
            setTimeout(() => {
                random.func();
            }, 1000);
        }
        
        function activateCosmicMode() {
            cosmicMode = !cosmicMode;
            
            if (cosmicMode) {
                document.body.style.animation = 'cosmicPulse 2s ease-in-out infinite';
                updateStatus('üåü COSMIC MODE ACTIVATED! REALITY ENHANCED!');
                
                // Enhanced visual effects
                setInterval(() => {
                    if (cosmicMode && Math.random() > 0.7) {
                        addFloatingElement(['‚ú®', 'üåü', 'üí´', 'üåå'][Math.floor(Math.random() * 4)]);
                    }
                }, 2000);
                
            } else {
                document.body.style.animation = 'cosmicPulse 8s ease-in-out infinite';
                updateStatus('üåå Cosmic mode deactivated. Reality restored to normal levels.');
            }
        }
        
        function showInfo() {
            showCosmicAlert('üåå COSMIC ENTERTAINMENT HUB INFO üåå\n\nüé≠ AI Comedy Lounge:\nInteractive AI comedian with cosmic humor, joke generation, and comedy chat.\n\nüé∞ Monster Kozmic Casino:\nUltimate absurdist gambling with sentient slot machines and cosmic events.\n\nüé∏ Kozmic Music Studios:\nProfessional music creation with portable, live, and master studio options.\n\nüåü Ultimate Experience:\nUnified access to all portals with enhanced cosmic features.\n\n‚ú® Created by cosmic developers for maximum interdimensional entertainment!\n\nPeace and Universal Harmony! œàŒ©¬ß‚àû');
        }
        
        function addFloatingElement(emoji) {
            const element = document.createElement('div');
            element.className = 'floating-element';
            element.textContent = emoji;
            element.style.left = Math.random() * window.innerWidth + 'px';
            element.style.top = window.innerHeight - 100 + 'px';
            
            document.body.appendChild(element);
            
            setTimeout(() => {
                if (document.body.contains(element)) {
                    document.body.removeChild(element);
                }
            }, 4000);
        }
        
        function updateStatus(message) {
            document.getElementById('statusBar').textContent = message;
            
            setTimeout(() => {
                document.getElementById('statusBar').textContent = 
                    'üåå Cosmic Entertainment Hub Online ‚Ä¢ All Portals Active ‚Ä¢ Reality Status: Stable (for now) ‚Ä¢ Peace and Universal Harmony! œàŒ©¬ß‚àû';
            }, 5000);
        }
        
        function logPortalAccess(portal) {
            portalActivations++;
            console.log(`[${new Date().toLocaleTimeString()}] Portal accessed: ${portal} (Total: ${portalActivations})`);
        }
        
        // Initialize the hub
        document.addEventListener('DOMContentLoaded', function() {
            updateStatus('üåå COSMIC ENTERTAINMENT HUB INITIALIZED! ALL PORTALS ONLINE!');
            
            // Random floating elements
            setInterval(() => {
                if (Math.random() > 0.8) {
                    addFloatingElement(['üé≠', 'üé∞', 'üé∏', 'üåü', '‚ú®'][Math.floor(Math.random() * 5)]);
                }
            }, 8000);
            
            console.log('üåå Cosmic Entertainment Hub loaded successfully!');
        });
        
        // Easter egg: Konami code for ultimate cosmic mode
        let konamiCode = [];
        const konamiSequence = ['ArrowUp', 'ArrowUp', 'ArrowDown', 'ArrowDown', 'ArrowLeft', 'ArrowRight', 'ArrowLeft', 'ArrowRight', 'KeyB', 'KeyA'];
        
        document.addEventListener('keydown', function(e) {
            konamiCode.push(e.code);
            if (konamiCode.length > 10) konamiCode.shift();
            
            if (konamiCode.join(',') === konamiSequence.join(',')) {
                showCosmicAlert('üåü KONAMI CODE ACTIVATED!\n\nüéÆ ULTIMATE COSMIC CHEAT MODE UNLOCKED!\n\nüí´ All portals now have maximum cosmic enhancement!\n‚ú® Reality distortion at maximum levels!\nüåå Congratulations, cosmic master!');
                
                activateCosmicMode();
                for (let i = 0; i < 30; i++) {
                    setTimeout(() => addFloatingElement('üåü'), i * 100);
                }
                
                konamiCode = [];
            }
        });
    </script>
